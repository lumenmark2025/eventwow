{
  "rewrites": [
    { "source": "/api/:path*", "destination": "/api/:path*" },
    { "source": "/sitemap.xml", "destination": "/api/sitemap.xml" },
    { "source": "/", "destination": "/index.html" },
    { "source": "/browse", "destination": "/index.html" },
    { "source": "/how-it-works", "destination": "/index.html" },
    { "source": "/pricing", "destination": "/index.html" },
    { "source": "/contact", "destination": "/index.html" },
    { "source": "/request", "destination": "/index.html" },
    { "source": "/request/:token", "destination": "/index.html" },
    { "source": "/enquiry/:token", "destination": "/index.html" },
    { "source": "/suppliers", "destination": "/index.html" },
    { "source": "/suppliers/:slug", "destination": "/index.html" },
    { "source": "/suppliers/:slug/request-quote", "destination": "/index.html" },
    { "source": "/categories", "destination": "/index.html" },
    { "source": "/categories/:slug", "destination": "/index.html" },
    { "source": "/category/:categorySlug", "destination": "/index.html" },
    { "source": "/location/:locationSlug", "destination": "/index.html" },
    { "source": "/:slug([a-z0-9]+(?:-[a-z0-9]+)+)", "destination": "/index.html" },
    { "source": "/venues", "destination": "/index.html" },
    { "source": "/venues/:slug", "destination": "/index.html" },
    { "source": "/venues/:slug/claim", "destination": "/index.html" },
    { "source": "/claim/venue", "destination": "/index.html" },
    { "source": "/login", "destination": "/index.html" },
    { "source": "/supplier/login", "destination": "/index.html" },
    { "source": "/reset-password", "destination": "/index.html" },
    { "source": "/update-password", "destination": "/index.html" },
    { "source": "/auth/callback", "destination": "/index.html" },
    { "source": "/auth/reset", "destination": "/index.html" },
    { "source": "/supplier/:path*", "destination": "/index.html" },
    { "source": "/venue/:path*", "destination": "/index.html" },
    { "source": "/admin/:path*", "destination": "/index.html" },
    { "source": "/quote/:token", "destination": "/index.html" }
  ]
}
